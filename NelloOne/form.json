{
  "elements": [
    {
      "type": "Image",
      "image": "data:image/png;base64,"
    },
    {
      "type": "Label",
      "caption": "Nello One - A project of Ulrich Bittner"
    },
    {
      "type": "Label",
      "caption": " "
    },
    {
      "type": "Label",
      "caption": "Description:"
    },
    {
      "type": "Label",
      "caption": "This instance integrates the Nello One, the secure keyless access upgrade for your intercom, into IP-Symcon."
    },
    {
      "type": "Label",
      "caption": "You can receive a notification when someone rings the doorbell and you can open the door."
    },
    {
      "type": "Label",
      "caption": "For further information, please refer to the documentation or the notes here in the configuration."
    },
    {
      "type": "Label",
      "caption": " "
    },
    {
      "type": "Label",
      "caption": "Configuration:"
    },
    {
      "type": "ExpansionPanel",
      "caption": "(0) Instance information",
      "items": [
        {
          "type": "Label",
          "caption": "Symcon version:\t5.1>"
        },
        {
          "type": "Label",
          "caption": "Module version:\t2.01-2001"
        },
        {
          "type": "Label",
          "caption": "Date:\t\t\tApril 19, 2019"
        },
        {
          "type": "Label",
          "caption": "Time:\t\t\t10:00"
        },
        {
          "type": "Label",
          "caption": "Developer:\t\tUlrich Bittner"
        },
        {
          "type": "Label",
          "caption": "Prefix:\t\t\tNELLO"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "(1) Token",
      "items": [
        {
          "type": "Label",
          "caption": "If no token is displayed, please use the button (1) Token REGISTER first or add your token manually."
        },
        {
          "name": "Token",
          "type": "ValidationTextBox",
          "caption": "Token"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "(2) Locations",
      "items": [
        {
          "type": "Label",
          "caption": "Please use the button (2) Locations IMPORT for automatic import or enter the data manually."
        },
        {
          "type": "Label",
          "caption": " "
        },
        {
          "type": "List",
          "name": "LocationList",
          "rowCount": 5,
          "add": true,
          "delete": true,
          "sort": {
            "column": "Position",
            "direction": "ascending"
          },
          "columns": [
            {
              "caption": "Position",
              "name": "Position",
              "width": "125px",
              "add": "",
              "edit": {
                "type": "ValidationTextBox"
              }
            },
            {
              "caption": "Utilisation",
              "name": "UseUtilisation",
              "width": "125px",
              "add": false,
              "edit": {
                "type": "CheckBox"
              }
            },
            {
              "caption": "Location ID",
              "name": "LocationID",
              "width": "350px",
              "add": "",
              "edit": {
                "type": "ValidationTextBox"
              }
            },
            {
              "caption": "Street",
              "name": "Street",
              "width": "250px",
              "add": "",
              "edit": {
                "type": "ValidationTextBox"
              }
            },
            {
              "caption": "City",
              "name": "City",
              "width": "250px",
              "add": "",
              "edit": {
                "type": "ValidationTextBox"
              }
            },
            {
              "caption": "Country",
              "name": "Country",
              "width": "250px",
              "add": "",
              "edit": {
                "type": "ValidationTextBox"
              }
            }
          ]
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "(3) Webhook",
      "items": [
        {
          "type": "Label",
          "caption": "You can receive status information from your Nello One automatically. Please use the (3) Webhook ADD/UPDATE button to create the webhook."
        },
        {
          "type": "Label",
          "caption": " "
        },
        {
          "type": "Label",
          "caption": "Please enter any user name for authentification."
        },
        {
          "name": "WebhookUsername",
          "type": "ValidationTextBox",
          "caption": "Username"
        },
        {
          "type": "Label",
          "caption": "Please enter any password for authentification."
        },
        {
          "name": "WebhookPassword",
          "type": "ValidationTextBox",
          "caption": "Password"
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "(4) Push Notification",
      "items": [
        {
          "type": "Label",
          "caption": "Please select the corresponding WebFront instance to receive notifications about status changes when this function is activated."
        },
        {
          "type": "Label",
          "caption": " "
        },
        {
          "type": "CheckBox",
          "name": "UseNotification",
          "caption": "Receive notifications"
        },
        {
          "type": "SelectInstance",
          "name": "WebFrontID",
          "caption": "WebFront Instance"
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "Label",
      "caption": "(0) Instance information"
    },
    {
      "type": "Button",
      "caption": "Manual",
      "onClick": "echo 'https://github.com/ubittner/SymconNello';"
    },
    {
      "type": "Label",
      "caption": "(1) Token"
    },
    {
      "type": "Button",
      "caption": "Register",
      "onClick": "echo NELLO_Register($id);"
    },
    {
      "type": "Label",
      "caption": "(2) Locations"
    },
    {
      "type": "RowLayout",
      "items": [
        {
          "type": "Button",
          "caption": "List",
          "onClick": "$locations = NELLO_GetLocations($id); print_r($locations);"
        },
        {
          "type": "Button",
          "caption": "Import",
          "onClick": "NELLO_ImportLocations($id);"
        },
        {
          "type": "Button",
          "caption": "Open door",
          "onClick": "NELLO_BuzzDoor($id);"
        }
      ]
    },
    {
      "type": "Label",
      "caption": "(3) Webhook"
    },
    {
      "type": "RowLayout",
      "items": [
        {
          "type": "Button",
          "caption": "Add/update",
          "onClick": "NELLO_AddUpdateWebhook($id);"
        },
        {
          "type": "Button",
          "caption": "Delete",
          "onClick": "NELLO_DeleteWebhook($id);"
        }
      ]
    }
  ],
  "status": [
    {
      "code": 101,
      "icon": "active",
      "caption": "Nello One will be created"
    },
    {
      "code": 102,
      "icon": "active",
      "caption": "Nello One is active"
    },
    {
      "code": 103,
      "icon": "active",
      "caption": "Nello One will be deleted"
    },
    {
      "code": 104,
      "icon": "inactive",
      "caption": "Nello One is inactive"
    },
    {
      "code": 201,
      "icon": "error",
      "caption": "An unknown error has occurred"
    },
    {
      "code": 2111,
      "icon": "error",
      "caption": "No token found under (1) Token. Please register for a token first or add existing token."
    },
    {
      "code": 2201,
      "icon": "error",
      "caption": "No data found under point (2) Locations. Please import the locations first."
    },
    {
      "code": 2211,
      "icon": "error",
      "caption": "Please check the configuration under point (2) Locations."
    },
    {
      "code": 2411,
      "icon": "error",
      "caption": "Please select a valid WebFrontID under point (4) Push Notification."
    }
  ]
}